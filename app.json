{
  "expo": {
    "name": "frontend-student",
    "slug": "frontend-student",
    "scheme": "frontend-student",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/adaptive-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#f3f3f1"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.frontendstudent",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "androidNavigationBar": {
      "barStyle": "light-content",
      "backgroundColor": "#f3f3f1"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.jpg",
        "backgroundColor": "#6366f1"
      },
      "package": "com.anonymous.frontendstudent"
    },
    "web": {
      "favicon": "./assets/logo.jpg"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Samarkan.ttf",
            "./assets/fonts/awesome.regular.ttf"
          ]
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "e377b310-dfe6-4ee2-8443-bfb8ab081e85"
      }
    }
  }
}
